    "normalize-css": "^2.3.1",
    "react": "16.13.1",
    "react-dom": "16.13.1",
    "react-router-dom": "^5.2.0",
    "regenerator-runtime": "0.13.7",
    "sass-loader": "9.0.2",
    "supertest": "^4.0.2",
    "webpack": "4.44.1",
    "webpack-cli": "3.3.12"
